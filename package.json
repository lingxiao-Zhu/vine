{
  "name": "vine-design",
  "version": "0.1.3",
  "description": "A React mobile UI design language ",
  "main": "build/dist/index.js",
  "scripts": {
    "start": "parcel server/index.html --open",
    "dev": "cross-env NODE_ENV=development rollup -c rollup.config.js",
    "build": "cross-env NODE_ENV=production BABEL_ENV=production rollup -c rollup.config.js && node scripts/generate-css-js.js",
    "test-all": "sh scripts/test-all.sh",
    "test": "cross-env BABEL_ENV=test jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lingxiao-Zhu/vine.git"
  },
  "keywords": [
    "vine",
    "react",
    "ui",
    "design",
    "framework",
    "component"
  ],
  "author": "Larry",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lingxiao-Zhu/vine/issues"
  },
  "homepage": "https://github.com/lingxiao-Zhu/vine#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^24.7.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.26.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.0.0",
    "node-sass": "^4.11.0",
    "react-test-renderer": "^16.8.6",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "3",
    "rollup-plugin-clear": "^2.0.7",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-eslint": "^5.1.0",
    "rollup-plugin-node-resolve": "^4.2.1",
